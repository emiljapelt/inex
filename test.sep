

struct tuple<T>(fst: T, snd: T)

intern t :tuple:= {1,2}

intern do ::= <T>(a: T, f: (T)) {
    f(a);
}

intern iter ::= <T>(a: T[], f: (T)) {
    for(i ::= 0; i < |a|; i +:= 1)
        f(a[i]);
}

