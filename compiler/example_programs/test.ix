
include ../library/collections/stack.ix;
include ../library/collections/tuple.ix;

external test() {
    
    s1: stack<int>;
    stack_from_array([1,2,3], s1);
    s2: stack<int>;
    stack_from_array([4,5,6], s2);

    s3: stack<tuple<int,int>>;
    zip(s1, s2, s3);

    printer(s3);
}

internal printer(target: stack<tuple<int,int>>) {

    pointer ::= target;
    until(pointer = null) {
        print pointer.top.fst, ' ', pointer.top.snd, '\n';
        pointer := pointer.rest;
    }

}

external meme() {

    arr :int:= 0;

    inner_meme(4, arr);

    print arr;

}

internal inner_meme<T>(elem: T, target: T) {
    target := $elem;
}