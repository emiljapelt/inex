locked int funny := 69;

external main(int funny_count) {

    until(funny_count = 0) {
        if (funny_count < 0) {
            print -funny;
            funny_count := funny_count + 1;
        }
        else {
            print funny;
            funny_count := funny_count - 1;
        }
    }

    for ( var i := 0 ; i < 22 ; i := i + 1 ) {
        var temp := i;
        fib(temp);   
    }
}

internal add(locked int a, locked int b, int res) {
    res := a + b;
}

internal sub(locked int a, locked int b, int res) {
    res := a - b;
}

external funny_time(int c) {
    while(!(c = 0)) {
        print funny;
        c := c - 1;
    }
}

external fib(int c) {
    int res;
    'fib(res, 1, c);
    print res;
}

internal 'fib(int a, int b, int c) {
    if (c <= 0) stop;
    var temp := a;
    a := b;
    b := temp + b;
    'fib(a, b, c-1);
}