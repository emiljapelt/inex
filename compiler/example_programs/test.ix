external test_arr() {

    a1 ::= [1,2,3];
    mod_arr(a1);
    print a1[0], '\n';
    print a1[1], '\n';
    print a1[2], '\n';

}

internal mod_arr(a: int[]) {
    a1 ::= $a;
    print a1[0], '\n';
    print a1[1], '\n';
    print a1[2], '\n';

    a1 := [4,5,6];
    print a1[0], '\n';
    print a1[1], '\n';
    print a1[2], '\n';
}


struct tuple<T,U>(fst: T, snd: U);

external test_str() {

    s1 ::= new tuple(1,2);
    mod_str(s1);
    print s1.fst, '\n';
    print s1.snd, '\n';

}

internal mod_str(s: tuple<int,int>) {
    s1 ::= $s;
    print s1.fst, '\n';
    print s1.snd, '\n';

    s1 := new tuple(3,4);
    print s1.fst, '\n';
    print s1.snd, '\n';
}