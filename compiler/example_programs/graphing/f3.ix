
struct list<T>(head: T, tail: list<T>);

external prepend<T>(element: T, target: list<T>) {
    target := { element, target };
}

meme: int;

external sum(l: list<int>, out: int) {
    out := 0;
    handle ::= $l;
    while(handle != null) {
        out +:= handle.head;
        handle := handle.tail;
    }
}